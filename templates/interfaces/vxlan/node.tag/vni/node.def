type: u32
help: Identificador de rede virtual [REQUERIDO]
syntax:expression: $VAR(@) >= 0 && $VAR(@) <= 16777216; "Deve ser entre 0-16777216"
val_help: u32: 0-16777214; Identificador de rede virtual dessa interface VXLAN [REQUERIDO]

update:
  if [ ! -e /tmp/vxlan-$VAR(../@)-create ]; then
    echo "Chainging vni requires delete/create this vxlan interface"
    exit 1
  fi
