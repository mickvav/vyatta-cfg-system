multi:
type: txt
help: Usar nomes de servidores recebidos pelo servidor DHCP na interface especificada
commit:expression: exec "/opt/vyatta/sbin/vyatta-dns-forwarding.pl --dhcp-interface $VAR(@)"
allowed:
         local -a array ;
         array=( /var/lib/dhcp/eth* /var/lib/dhcp/br* ) ;
         echo  -n ${array[@]##*/}
