type: u32
help: Intervalo em que os enderecos sao preservados
default: 300
syntax:expression: $VAR(@) == 0 || ( $VAR(@) >= 10 && $VAR(@) < 1000000 ) ;
	"Bridge aging time must be between 10 and 1000000 or zero"
val_help: 0; Desabilitar intervalo de preservacao de endereco (sempre alterar)
val_help: u32:10-1000000; Tempo de envelhecimento para endereco bridge em segundos (padrao 300)

update: /sbin/brctl setageing $VAR(../@) $VAR(@)
delete: /sbin/brctl setageing $VAR(../@) 300
