type: u32
help: Intervalo no qual vizinhos bridge sao removidos
default: 20
syntax:expression: $VAR(@) >= 6 && $VAR(@) <= 200 ;
	"Valor de intervalo deve ser entre 6 e 200"
val_help: u32:6-200; Valor de tempo maximo em segundos (default 20)

update: /sbin/brctl setmaxage $VAR(../@) $VAR(@)
delete: /sbin/brctl setmaxage $VAR(../@) 20

