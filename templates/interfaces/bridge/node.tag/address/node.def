multi:
priority: 320
type: txt
help: Endereco IP

syntax:expression: exec "/opt/vyatta/sbin/valid_address $VAR(@)"

create: sudo /opt/vyatta/sbin/vyatta-address add $VAR(../@) $VAR(@)

delete: sudo /opt/vyatta/sbin/vyatta-address delete $VAR(../@) $VAR(@)

allowed: echo "dhcp <>"
val_help: ipv4net; Endereco IP e comprimento de prefixo
val_help: ipv6net; Endereco IPv6 e comprimento de prefixo
val_help: dhcp; DHCP
val_help: dhcpv6; DHCPv6
